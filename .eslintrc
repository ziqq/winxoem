{
    "extends": ["prettier"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    },

    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "rules": {
        "jsx-quotes": [
            1,
            "prefer-double"
        ]
    },
    "plugins": [
        "prettier"
    ]

}
