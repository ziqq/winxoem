{
    "name": "beautybox",
    "version": "0.0.3",
    "author": "Anton Ustinoff <a.a.ustinoff@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/SPITIRU/beautybox-markup"
    },
    "scripts": {
        "ghpages": "git subtree push --prefix build origin gh-pages",
        "dev": "gulp",
        "build": "gulp build --production",
        "precommit": "lint-staged",
        "commit": "git-cz",
        "eslint": "eslint --format=node_modules/eslint-formatter-pretty src/js/",
        "eslint-fix": "npm run eslint -- --fix",
        "style:lint": "stylelint src/sass/**/*.scss --config ./.stylelintrc",
        "style:fix": "stylelint src/sass/**/*.scss --fix --config ./.stylelintrc",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "lint-staged": {
        "linters": {
            "src/templates/**/*.html": [
                "htmlhint --config .htmlhintrc"
            ],
            "src/js/**/*.{js, jsx}": [
                "node_modules/.bin/eslint",
                "prettier --write",
                "git add"
            ],
            "src/sass/**/*.scss": [
                "stylelint --fix --config ./.stylelintrc",
                "prettier --write",
                "git add"
            ]
        }
    },
